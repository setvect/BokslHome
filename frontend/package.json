    "@codemirror/language": "^6.11.2",
    "@codemirror/legacy-modes": "^6.5.1",
    "@codemirror/theme-one-dark": "^6.1.3",
    "@lezer/common": "^1.2.3",
    "@lezer/highlight": "^1.2.1",
    "@types/prismjs": "^1.26.5",
    "formsnap": "^2.0.1",
    "marked": "^16.1.2",
    "mermaid": "^11.9.0",
    "prismjs": "^1.30.0",
    "svelte-codemirror-editor": "^1.4.1",
    "sveltekit-superforms": "^2.27.1",
